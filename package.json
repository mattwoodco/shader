{
  "name": "start",
  "private": true,
  "scripts": {
    "dev": "yarn schema & next dev",
    "build": "next build",
    "start": "next start",
    "up": "docker compose up -d db",
    "down": "docker compose down && rm -rf data",
    "types": "tsc --noEmit",
    "lint": "next lint",
    "email": "docker compose up -d email",
    "generate": "dotenv -e .env.local -- prisma generate",
    "migrate": "dotenv -e .env.local -- prisma migrate dev",
    "clean": "rm -rf lib/prisma/migrations",
    "pull": "dotenv -e .env.local -- prisma db pull",
    "reset": "dotenv -e .env.local -- prisma migrate reset",
    "schema": "dotenv -e .env.local -- prisma generate --watch",
    "seed": "dotenv -e .env.local -- prisma db seed",
    "studio": "dotenv -e .env.local -- prisma studio",
    "newdb": "yarn clean && yarn migrate && yarn seed && yarn studio",
    "yoloupdate": "npx taze major -r -w && yarn",
    "nuke": "rm -rf .git && git init --quiet && git remote add origin https://github.com/mattwoodco/workbench && git add . && git c init && git pu --force"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.6",
    "@next-auth/prisma-adapter": "^1.0.4",
    "@prisma/client": "^4.3.1",
    "@supabase/supabase-js": "^1.35.6",
    "@tanstack/react-query": "^4.2.3",
    "airtable": "^0.11.4",
    "framer-motion": "^7.2.1",
    "glslCanvas": "^0.1.7",
    "ky": "^0.31.3",
    "next": "latest",
    "next-auth": "latest",
    "next-connect": "next",
    "next-seo": "^5.5.0",
    "nodemailer": "^6.7.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.34.2",
    "react-icons": "^4.4.0",
    "react-loader-spinner": "^5.3.3",
    "zustand": "^4.1.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@rvxlab/tailwind-plugin-ios-full-height": "^1.1.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.7",
    "@types/node": "18.7.14",
    "@types/nodemailer": "^6.4.5",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "^10.4.8",
    "daisyui": "^2.24.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "8.23.0",
    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "prisma": "^4.3.1",
    "sass": "^1.54.8",
    "tailwind-scrollbar-hide": "^1.1.7",
    "tailwindcss": "^3.1.8",
    "tailwindcss-fluid-type": "^1.3.3",
    "tailwindcss-golden-ratio": "^0.4.0",
    "tailwindcss-unsplash": "^1.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "prisma": {
    "schema": "lib/prisma/schema.prisma",
    "seed": "ts-node lib/prisma/seed/seed.ts"
  }
}
